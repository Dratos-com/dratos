// Hierarchical Task Network (HTN) Grammar

start: htn

htn: task_network+

task_network: compound_task | primitive_task

compound_task: IDENTIFIER ":" subtasks

subtasks: "{" task_network+ "}"

primitive_task: IDENTIFIER "(" parameters? ")"

parameters: IDENTIFIER ("," IDENTIFIER)*

IDENTIFIER: /[a-zA-Z_][a-zA-Z0-9_]*/

%import common.WS
%ignore WS
